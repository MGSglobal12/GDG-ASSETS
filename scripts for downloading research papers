      "cell_type": "code",
      "source": [
        "import re, time, requests, os\n",
        "\n",
        "def sanitize(name):\n",
        "    return re.sub(r'[\\\\/*?:\"<>|]', \"_\", name)\n",
        "\n",
        "retry_count = 0\n",
        "for item in log[\"failed\"]:\n",
        "    doi = item.get(\"doi\", \"\")\n",
        "    label = item.get(\"label\", \"\")\n",
        "    filepath = os.path.join(OUTPUT_FOLDER, sanitize(label) + \".pdf\")\n",
        "\n",
        "    if not doi:\n",
        "        continue\n",
        "\n",
        "    # Try fetching the DOI page directly\n",
        "    try:\n",
        "        headers = {\"User-Agent\": \"Mozilla/5.0\"}\n",
        "        r = requests.get(f\"https://doi.org/{doi}\", timeout=30,\n",
        "                        headers=headers, allow_redirects=True)\n",
        "        if r.status_code == 200 and len(r.content) > 5000:\n",
        "            content_type = r.headers.get(\"Content-Type\", \"\")\n",
        "            if \"pdf\" in content_type.lower():\n",
        "                with open(filepath, \"wb\") as f:\n",
        "                    f.write(r.content)\n",
        "                retry_count += 1\n",
        "                print(f\"‚úÖ Recovered: {label}\")\n",
        "    except:\n",
        "        pass\n",
        "    time.sleep(1.5)\n",
        "\n",
        "print(f\"\\nüîÅ Recovered {retry_count} additional PDFs!\")"
      ],
      "metadata": {
        "colab": {
          "base_uri": "https://localhost:8080/"
        },
