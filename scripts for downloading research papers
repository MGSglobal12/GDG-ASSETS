      "source": [
        "import requests, os, re, time\n",
        "\n",
        "def sanitize(name):\n",
        "    return re.sub(r'[\\\\/*?:\"<>|]', \"_\", name)\n",
        "\n",
        "def save_pdf(content, label):\n",
        "    path = os.path.join(OUTPUT_FOLDER, sanitize(label) + \".pdf\")\n",
        "    with open(path, \"wb\") as f:\n",
        "        f.write(content)\n",
        "    print(f\"  ‚úÖ Saved: {label}\")\n",
        "    return path\n",
        "\n",
        "def try_download(url, label):\n",
        "    headers = {\"User-Agent\": \"Mozilla/5.0\"}\n",
        "    try:\n",
        "        r = requests.get(url, headers=headers, timeout=30, allow_redirects=True)\n",
        "        if r.status_code == 200 and len(r.content) > 5000:\n",
        "            ct = r.headers.get(\"Content-Type\", \"\")\n",
        "            if \"pdf\" in ct.lower():\n",
        "                save_pdf(r.content, label)\n",
        "                return True\n",
        "    except: pass\n",
        "    return False\n",
        "\n",
        "# ‚îÄ‚îÄ Targeted free sources ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ\n",
