      "cell_type": "code",
      "source": [
        "import re, time, json, requests\n",
        "from tqdm.notebook import tqdm\n",
        "\n",
        "MISSED_FILE = \"/content/missed_papers.txt\"\n",
        "log = {\"downloaded\": [], \"not_found\": [], \"failed\": []}\n",
        "counts = {\"downloaded\": 0, \"not_found\": 0, \"failed\": 0}\n",
        "\n",
        "def sanitize(name):\n",
        "    return re.sub(r'[\\\\/*?:\"<>|]', \"_\", name)\n",
        "\n",
        "def query_unpaywall(doi):\n",
        "    try:\n",
        "        r = requests.get(f\"https://api.unpaywall.org/v2/{doi}?email={YOUR_EMAIL}\", timeout=15)\n",
        "        if r.status_code == 200:\n",
        "            loc = r.json().get(\"best_oa_location\")\n",
        "            if loc:\n",
        "                return loc.get(\"url_for_pdf\") or loc.get(\"url\")\n",
        "    except: pass\n",
        "    return None\n",
        "\n",
        "def download_pdf(url, path):\n",
        "    try:\n",
        "        r = requests.get(url, timeout=30, headers={\"User-Agent\": \"Mozilla/5.0\"}, allow_redirects=True)\n",
        "        if r.status_code == 200 and len(r.content) > 5000:\n",
        "            with open(path, \"wb\") as f: f.write(r.content)\n",
        "            return True, len(r.content)\n",
        "    except: pass\n",
        "    return False, 0\n",
        "\n",
        "for doi, label in tqdm(PAPERS, desc=\"Downloading\", unit=\"paper\"):\n",
        "    filepath = os.path.join(OUTPUT_FOLDER, sanitize(label) + \".pdf\")\n",
        "\n",
        "    if os.path.exists(filepath) and os.path.getsize(filepath) > 5000:\n",
        "        counts[\"downloaded\"] += 1\n",
        "        continue\n",
        "\n",
        "    if not doi:\n",
        "        counts[\"not_found\"] += 1\n",
        "        log[\"not_found\"].append({\"label\": label})\n",
        "        continue\n",
        "\n",
        "    pdf_url = query_unpaywall(doi)\n",
        "    time.sleep(1.0)\n",
        "\n",
        "    if not pdf_url:\n",
        "        counts[\"not_found\"] += 1\n",
        "        log[\"not_found\"].append({\"doi\": doi, \"label\": label, \"url\": f\"https://doi.org/{doi}\"})\n",
        "        continue\n",
        "\n",
        "    ok, size = download_pdf(pdf_url, filepath)\n",
        "    if ok:\n",
        "        counts[\"downloaded\"] += 1\n",
        "        log[\"downloaded\"].append({\"label\": label, \"size_kb\": round(size/1024,1)})\n",
        "    else:\n",
        "        counts[\"failed\"] += 1\n",
        "        log[\"failed\"].append({\"doi\": doi, \"label\": label, \"url\": f\"https://doi.org/{doi}\"})\n",
        "\n",
        "# Save missed papers list\n",
        "missed = log[\"not_found\"] + log[\"failed\"]\n",
        "with open(MISSED_FILE, \"w\") as f:\n",
        "    f.write(\"MISSED PAPERS ‚Äî Download via your university library\\n\")\n",
        "    f.write(\"=\" * 55 + \"\\n\\n\")\n",
        "    for i, p in enumerate(missed, 1):\n",
        "        f.write(f\"[{i}] {p.get('label','N/A')}\\n\")\n",
        "        if \"url\" in p: f.write(f\"    {p['url']}\\n\")\n",
        "        f.write(\"\\n\")\n",
        "\n",
        "print(f\"\\n‚úÖ Downloaded:      {counts['downloaded']} / {len(PAPERS)}\")\n",
        "print(f\"‚ùå Not open-access: {counts['not_found']}\")\n",
        "print(f\"‚ö†Ô∏è  Failed:          {counts['failed']}\")\n",
        "print(f\"\\nüìÇ PDFs are in:     {OUTPUT_FOLDER}\")"
      ],
